--
-- TOC entry 218 (class 1259 OID 16587)
-- Name: order_details; Type: VIEW; Schema: public; Owner: pgadmin
--

CREATE VIEW public.order_details AS
 SELECT orders.id AS order_id,
    orders.order_date,
    order_item.product_id
   FROM (public.orders
     LEFT JOIN public.order_item ON ((orders.id = order_item.order_id)))
  WHERE ((orders.order_date + 90) > now());


ALTER TABLE public.order_details OWNER TO pgadmin;

