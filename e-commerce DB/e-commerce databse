-- TOC entry 219 (class 1259 OID 16591)
-- Name: product_order_details; Type: VIEW; Schema: public; Owner: pgadmin
--

CREATE VIEW public.product_order_details AS
 SELECT order_details.order_id,
    order_details.order_date,
    order_details.product_id,
    product.category_id
   FROM (public.order_details
     LEFT JOIN public.product ON ((order_details.product_id = product.id)));


ALTER TABLE public.product_order_details OWNER TO pgadmin;

--
-- TOC entry 220 (class 1259 OID 16595)
-- Name: category_order_details; Type: VIEW; Schema: public; Owner: pgadmin
--

CREATE VIEW public.category_order_details AS
 SELECT product_category.id,
    product_category.name,
    product_order_details.order_date,
    product_order_details.order_id
   FROM (public.product_category
     RIGHT JOIN public.product_order_details ON ((product_category.id = product_order_details.category_id)));


ALTER TABLE public.category_order_details OWNER TO pgadmin;

